   <children>
      <Label layoutX="43.0" layoutY="21.0" text="Customers">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="customerTableView" layoutX="35.0" layoutY="64.0" prefHeight="218.0" prefWidth="758.0">
